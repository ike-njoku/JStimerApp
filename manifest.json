{
    "manifest_version": 2,
    "name": "Timer",
    "version": "1.0.0",
    "description": " timer: sets a countdownt timer and alerts you when the countdown is exhausted ",


    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "16.png"

    },

    "icons": {
        "128": "128.png"
    },

    "permissions": ["activeTab"]
}